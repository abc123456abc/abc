	-------------------ProcessLocking


import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

/**
 * @author sivaiah.polamreddy
 * 
 */
@Entity
@Table(name = "PROCESS_LOCKING")
public class ProcessLocking implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@Column(name = "PRODUCTID")
	private String productId;
	
	@Column(name = "PROCESSID")
	private String processId;
	
	@Column(name = "PRODUCTNAME")
	private String productName;
	
	@Column(name = "RECORDTIME")
	private Date createTime;
	
	@Column(name = "STATUS")
	private Integer status;

	@Column(name = "CPROCESSID")
	private String cProcessId;
	
	public String getProductId() {
		return productId;
	}

	public void setProductId(String productId) {
		this.productId = productId;
	}

	public String getProcessId() {
		return processId;
	}

	public void setProcessId(String processId) {
		this.processId = processId;
	}

	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getcProcessId() {
		return cProcessId;
	}

	public void setcProcessId(String cProcessId) {
		this.cProcessId = cProcessId;
	}

}
// create table PROCESS_LOCKING(PRODUCTID VARCHAR2(32),PROCESSID VARCHAR2(32),PRODUCTNAME VARCHAR2(50),RECORDTIME DATE DEFAULT SYSDATE,STATUS number);
===============================End===========================


	------------------
RemarkBean


import java.io.Serializable;

@SuppressWarnings("serial")
public class RemarkBean implements Serializable {
	String by;
	String rtimestamp;
	String text;
	
	
	public String getBy() {
		return by;
	}


	public void setBy(String by) {
		this.by = by;
	}


	public String getRtimestamp() {
		return rtimestamp;
	}


	public void setRtimestamp(String rtimestamp) {
		this.rtimestamp = rtimestamp;
	}


	public String getText() {
		return text;
	}


	public void setText(String text) {
		this.text = text;
	}


	public String toJSON(){
		return "{"+"by:"+"\""+by+"\","
				+"timestamp:"+"\""+rtimestamp+"\","+"text:"+"\""+text+"\","+"}";
	}
}

============================ End========================

